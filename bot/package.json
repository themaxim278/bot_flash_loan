    "fork:simulate": "ts-node ./scripts/run-fork-sim.ts",
    "execute": "node dist/cli.js execute",
    "test": "vitest run"
  },
  "dependencies": {
    "better-sqlite3": "^12.4.1",
    "dotenv": "^16.4.5",
    "ethers": "^6.13.2",
    "pino": "^9.4.0",
    "prom-client": "^15.1.3",
    "yaml": "^2.4.5",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.13",
    "ts-node": "^10.9.2",
    "vitest": "^1.6.0"
